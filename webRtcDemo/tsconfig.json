{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.wdm/*"],
      "wdmjs": ["src/.wdm"],
      "wdmjs/typings": ["src/.wdm/typings"]
    },
    "allowSyntheticDefaultImports": true
  }
}
